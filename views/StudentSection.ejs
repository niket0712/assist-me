    </article>
    <nav id="mainNav">
        <div>
            Welcome back
        </div>

        <div>
            <form action="logout" method="post">
                <button class="btn btn-primary" type="submit"> Logout </button>
            </form>
        </div>
    </nav>
    <!-- <div id="siteAds">Ads</div> -->
    <footer id="pageFooter">
        <div class="text-center  mt-4 pt-2">
            <a href="StudentQuestion">
